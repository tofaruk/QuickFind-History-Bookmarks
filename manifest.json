{
  "manifest_version": 3,
  "name": "QuickFind: History, Bookmarks & Tabs",
  "description": "Quickly search and access your browsing history, bookmarks, and open tabs.",
  "version": "2.0.1",
  "action": {
    "default_title": "QuickFind",
    "default_popup": "popup.html"
  },
  "permissions": ["history", "bookmarks", "storage", "tabs", "favicon"],
  "web_accessible_resources": [
    {
      "resources": ["_favicon/*"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
